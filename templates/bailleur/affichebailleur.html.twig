    commission d'engagement : {{bailleur.commissionEngagement}} % </br>
    commission de service : {{bailleur.CommissionService}} % </br>
    commission initiale : {{bailleur.CommissionInitiale}} </br>
    commission d'arrangement : {{bailleur.CommissionArrangement}}</br>
    commission d'agent : {{bailleur.commissionAgent}} </br>
    maturite de la lettre de credit : {{bailleur.MaturiteLettreCredit}}</br>
    frais lies a la mise en place de la lettre de credit : {{bailleur.FraisLiesLettreCredit}}</br>
    frais lies au refinancement de la lettre de credit : {{bailleur.FraisLiesRefinancement}}</br>
    <hr>
    element-don : {{bailleur.Elementdon}} %</br>
