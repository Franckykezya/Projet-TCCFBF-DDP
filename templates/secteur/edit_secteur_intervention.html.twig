<!-- BOOTSTRAP STYLES-->
<link href="../assets/css/bootstrap.css" rel="stylesheet" />
<!-- BOOTSTRAP SCRIPTS -->
<script src="../assets/js/bootstrap.min.js"></script>


  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Modification secteur</h5>
        <a class="close" href="{{ path('secteur_liste') }}">&times;</a>
      </div>
      <div class="modal-body">
        <p>
            {{ form_start(form) }}
                <b> Nom du secteur :</b> {{ form_widget(form.nom, {'label' : 'Nom du secteur' , 'attr' : {'placeholder':"Nom du type financement"}}) }}
        </p>
      </div>
      <div class="modal-footer">
        <button class="btn btn-primary">
            {{ button_label|default('Enregistrer') }}
        </button>
        <a href="{{ path('secteur_liste') }}" type="button" class="btn btn-secondary" data-dismiss="modal">Annuler</a>
        {{ form_end(form) }}  
      </div>
    </div>
  </div>